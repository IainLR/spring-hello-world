pipeline{
  agent{ label 'linux' }
  tools{ 
    maven '3.8.2'
  }
  stages{
    stage("checkout"){
          steps{
            git branch: 'main', url: '${gitRepo}'
          }
    }
    stage("build"){
        steps{
            sh 'mvn clean package'
        }
    }
    stage("archive"){
        steps{
            archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
        }
    }
  }
}
