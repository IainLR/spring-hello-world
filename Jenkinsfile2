pipeline{
  agent{ label 'linux' }
  tools{ 
    maven '3.8.2'
  }
  steps{
    step("checkout"){
          git branch: 'main', url: '${gitRepo}'
    }
    step("build"){
        sh 'mvn clean package'
    }
    step("archive"){
        archiveArtifacts artifacts: 'target/*.jar', followSymlinks: false
    }
  }
}
